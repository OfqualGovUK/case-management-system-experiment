{
  "name": "ofqual_carbon",
  "private": true,
  "scripts": {
    "livereload": "livereload --exclusions node_modules/ --exts 'css,js,twig,theme,apng,avif,gif,jpg,jpeg,jfif,pjpeg,pjp,png,svg,webp'",
    "build:css": "sass --load-path=node_modules src/scss/style.scss build/css/style.css --no-source-map --style=expanded",
    "watch:css": "sass --watch --load-path=node_modules src/scss/style.scss build/css/style.css --style=expanded",
    "build:js": "webpack --mode production",
    "watch:js": "webpack --watch --mode development",
    "build": "npm run build:css && npm run build:js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "babel-loader": "^10.0.0",
    "livereload": "^0.9.3",
    "sass": "^1.92.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@carbon/grid": "^11.42.0",
    "@carbon/icon-helpers": "^10.65.0",
    "@carbon/icons": "^11.66.0",
    "@carbon/styles": "^1.89.0",
    "@carbon/web-components": "^2.37.0",
    "carbon-components": "^10.59.2"
  },
  "description": ""
}
